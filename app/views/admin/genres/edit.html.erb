<%= render 'admin/admins'%>

<div class="container" style="height: 500px;">
 <div class="row p-4">
  <h5 class="bg-light px-4">ジャンル編集</h5>
 </div>
 
 <div class="inline">
  <%= form_with model: @genre,url: admin_genre_path(@genre.id),method: :patch do |f| %>
   <label class="m-3" for="genre_name">ジャンル名</label>
   <%= f.text_field :name,class:'m-3' %>
   <%= f.submit '変更を保存',class:'btn btn-success m-3' %>
  <% end %>
 </div>
</div>