<%= render 'admin/admins' %>

<p>会員一覧</p>
<table>
 <thead>
  <tr>
   <td>会員ID</td>
   <td>氏名</td>
   <td>メールアドレス</td>
   <td>ステータス</td>
  </tr>
 </thead>
 
 <tbody class="table">
  <% @users.each do |user| %>
  <tr>
   <td><%= user.id %></td>
   <td>
    <%= link_to admin_customer_path(user.id) do %>
     <%= user.last_name %><%= user.first_name %>
    <% end %>
   </td>
   <td><%= user.email %></td>
   <td><%= user.is_active ? '有効' : '退会' %></td>
  </tr>
  <% end %>
 </tbody>
</table>